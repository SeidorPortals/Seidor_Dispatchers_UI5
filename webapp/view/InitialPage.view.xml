<mvc:View 
	controllerName="com.seidor.usa.dispatchers.controller.InitialPage"
	class="sapUiSizeCompact"
	xmlns="sap.m" 
	xmlns:core="sap.ui.core" 
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns:l="sap.ui.layout">
	<App id="idApp">
		<Page id="idPage" title="{i18n>title}">
			<content>
				<Carousel id="idCarousel" class="sapUiContentPadding" loop="true">
					<pages>
						<Image id="idImageSeidor" src="./image/seidor.png" alt="ECYO" width="25%"/>
						<Image id="idImageSAP" src="./image/sap.png" alt="SAP" width="25%"/>
					</pages>
				</Carousel>
			</content>
		</Page>
		
	</App>
</mvc:View>